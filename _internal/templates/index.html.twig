{% extends 'base.html.twig' %}

{% block content %}
    <div class="flex-container">
        <div class="file-info">
            <div id="icon"></div>
            <div>Name</div>
        </div>
        {% for file in files %}
            <div class="file-info">
                <div id="icon"><i class="{{ file.icon }}"></i></div>
                <div>{{ file.name }}</div>
            </div>
        {% endfor %}
    </div>
{% endblock %}